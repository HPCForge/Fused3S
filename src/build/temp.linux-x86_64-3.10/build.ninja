ninja_required_version = 1.3
cxx = /opt/apps/gcc/11.2.0/bin/g++
nvcc = /pub/zitongl5/cuda12.1/bin/nvcc

cflags = -Wno-unused-result -Wsign-compare -DNDEBUG -g -fwrapv -O3 -Wall -I/opt/apps/gcc/11.2.0/include -I/opt/apps/gcc/11.2.0/lib/gcc/x86_64-pc-linux-gnu/11.2.0/plugin/include -fPIC -I/dfs6/pub/zitongl5/DTCSpMM/lib/python3.10/site-packages/torch/include -I/dfs6/pub/zitongl5/DTCSpMM/lib/python3.10/site-packages/torch/include/torch/csrc/api/include -I/dfs6/pub/zitongl5/DTCSpMM/lib/python3.10/site-packages/torch/include/TH -I/dfs6/pub/zitongl5/DTCSpMM/lib/python3.10/site-packages/torch/include/THC -I/pub/zitongl5/cuda12.1/include -I/dfs6/pub/zitongl5/DTCSpMM/include -I/opt/apps/python/3.10.2/include/python3.10 -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=TCFMM -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++17
cuda_cflags = -I/dfs6/pub/zitongl5/DTCSpMM/lib/python3.10/site-packages/torch/include -I/dfs6/pub/zitongl5/DTCSpMM/lib/python3.10/site-packages/torch/include/torch/csrc/api/include -I/dfs6/pub/zitongl5/DTCSpMM/lib/python3.10/site-packages/torch/include/TH -I/dfs6/pub/zitongl5/DTCSpMM/lib/python3.10/site-packages/torch/include/THC -I/pub/zitongl5/cuda12.1/include -I/dfs6/pub/zitongl5/DTCSpMM/include -I/opt/apps/python/3.10.2/include/python3.10 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -O2 -lineinfo -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=TCFMM -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_80,code=sm_80 -ccbin /opt/apps/gcc/11.2.0/bin/gcc -std=c++17
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc --generate-dependencies-with-compile --dependency-output $out.d $cuda_cflags -c $in -o $out $cuda_post_cflags





build /dfs6/pub/zitongl5/TCFMM/src/build/temp.linux-x86_64-3.10/TCFMM.o: compile /dfs6/pub/zitongl5/TCFMM/src/TCFMM.cpp
build /dfs6/pub/zitongl5/TCFMM/src/build/temp.linux-x86_64-3.10/TCFMM_kernel.o: cuda_compile /dfs6/pub/zitongl5/TCFMM/src/TCFMM_kernel.cu






